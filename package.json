{
  "name": "backend-challenge-v2",
  "version": "1.0.0",
  "description": "Phone App",
  "main": "dist/main.js",
  "scripts": {
    "postci": "npm run build",
    "dev": "concurrently 'npm run watch:build:ts' 'npm run dev:runner'",
    "dev:runner": "node ./node_modules/.bin/moleculer-runner --hot -c moleculer.config.dev.json dist/services",
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "watch:build:ts": "npm run build:ts -- -w",
    "test": "jest"
  },
  "author": "katsanva <this.is.ftm@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.42",
    "@types/jest": "^23.3.5",
    "@types/mongoose": "^5.2.19",
    "@types/ramda": "^0.25.39",
    "concurrently": "^4.0.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "bignumber.js": "^7.2.1",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "helmet": "^3.14.0",
    "js-yaml": "^3.12.0",
    "moleculer": "^0.13.3",
    "moleculer-db": "^0.7.5",
    "moleculer-db-adapter-mongoose": "^0.7.1",
    "moleculer-web": "^0.8.2",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "ramda": "^0.25.0"
  },
  "peerDependencies": {
    "fastest-validator": "^0.6.10"
  },
  "engines": {
    "node": "^10.12.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
